{
  "name": "@kaiko.io/rescript-heroicons-react",
  "version": "2.0",
  "description": "Unofficial ReScript binding to @heroicons/react",
  "author": "Kaiko Systems GmbH",
  "contributors": [
    {
      "name": "Nyi Nyi Than",
      "email": "nyinyithann@gmail.com"
    },
    {
      "name": "Manuel Vazquez Acosta",
      "url": "https://twitter.com/mvaled"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/kaiko-systems/rescript-heroicons-react"
  },
  "license": "MIT",
  "keywords": [
    "rescript",
    "heroicons",
    "react",
    "tailwindCSS",
    "tailwind"
  ],
  "dependencies": {
    "@heroicons/react": "^2.0",
    "@rescript/react": "^0.11.0",
    "react": "^18.0"
  },
  "devDependencies": {
    "esbuild": "^0.17.16",
    "fzf": "^0.5.1",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "rescript": "^10.1.4",
    "rescript-webapi": "^0.7.0"
  },
  "scripts": {
    "generate": "node scripts/builder.js; node scripts/inject-tests.js",
    "build": "rescript build -with-deps && esbuild --target=chrome80,firefox80 --bundle tests/index.bs.js --outfile=tests/index.js",
    "run": "rescript build -with-deps && esbuild tests/index.bs.js --bundle --target=firefox80,chrome80 --servedir=tests --outfile=tests/index.js",
    "clean": "rescript clean",
    "watch": "rescript build -w",
    "iv": "npm --no-git-tag-version version patch",
    "verdaccio-publish": "npm run iv & npm publish --registry http://localhost:4873",
    "verdaccio-unpublish": "npm unpublish --force --registry http://localhost:4873/  rescript-heroicons"
  }
}
